        <!--<queryParams>str1234</queryParams>-->
        <handleConsumerURLAfterSLO>true</handleConsumerURLAfterSLO>
        <!--<redirectPathAfterSLO>http://www.xsd2xml.com</redirectPathAfterSLO>-->
        <applicationServerURL>https://localhost:8443</applicationServerURL>
        <saml>
            <enableSAMLSSO>true</enableSAMLSSO>
            <idpURL>https://localhost:9443/samlsso</idpURL>
            <idpEntityId>localhost</idpEntityId>
            <httpBinding>urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST</httpBinding>
            <!--<issuerId>str1234</issuerId>
            <consumerURL>str1234</consumerURL>-->
            <!--<attributeConsumingServiceIndex>str1234</attributeConsumingServiceIndex>-->
            <enableSLO>true</enableSLO>
            <consumerURLPostFix>/acs</consumerURLPostFix>
            <requestURLPostFix>samlsso</requestURLPostFix>
            <sloURLPostFix>logout</sloURLPostFix>
            <enableAssertionEncryption>false</enableAssertionEncryption>
            <enableAssertionSigning>false</enableAssertionSigning>
            <enableRequestSigning>false</enableRequestSigning>
            <enableResponseSigning>false</enableResponseSigning>
            <signatureValidatorImplClass>
                org.wso2.appserver.webapp.security.sso.saml.signature.SAMLSignatureValidatorImplementation
            </signatureValidatorImplClass>
            <!--<additionalRequestParams>str1234</additionalRequestParams>-->
            <!--<postBindingRequestHTMLPayload>str1234</postBindingRequestHTMLPayload>-->
            <isForceAuthn>false</isForceAuthn>
            <isPassiveAuthn>false</isPassiveAuthn>
            <keystorePath>
                file:///home/chiranga/wso2-packs/wso2-carbon/wso2carbon-4.4.0/repository/resources/security/wso2carbon.jks
            </keystorePath>
            <keystorePassword>wso2carbon</keystorePassword>
            <idpCertificateAlias>wso2carbon</idpCertificateAlias>
            <privateKeyAlias>wso2carbon</privateKeyAlias>
            <privateKeyPassword>wso2carbon</privateKeyPassword>
        </saml>
    </singleSignOn>
